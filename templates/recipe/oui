                <td>{{ recipe.name }}</td>
                <td>{{ recipe.ingredient ? recipe.ingredient|join(', ') : '' }}</td>
                <td>{{ recipe.equipement ? recipe.equipement|join(', ') : '' }}</td>
                <td>{{ recipe.content }}</td>
                <td>{{ recipe.duration }}</td>
                <td>{{ recipe.difficulty }}</td>
                <td>{{ recipe.conservation }}</td>
                <td>{{ recipe.image }}</td>
                <td>{{ recipe.status ? 'Yes' : 'No' }}</td>
                <td>{{ recipe.createdAt ? recipe.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ recipe.updatedAt ? recipe.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
